# TradingAgents Web Application Environment Variables

# API Keys (Required for full functionality)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GROQ_API_KEY=your_groq_api_key_here

# Backend Configuration
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8003
BACKEND_WORKERS=1
BACKEND_LOG_LEVEL=INFO

# Frontend Configuration
FRONTEND_PORT=80
VITE_API_BASE_URL=http://localhost:8003
VITE_WS_BASE_URL=ws://localhost:8003

# Security Configuration
CORS_ORIGINS=http://localhost:3000,http://localhost:5173,http://localhost:5174,http://localhost:80
RATE_LIMIT_REQUESTS_PER_MINUTE=60
SESSION_TIMEOUT_MINUTES=60

# Performance Configuration
MAX_WEBSOCKET_CONNECTIONS=100
MAX_SESSIONS=50
MESSAGE_BATCH_SIZE=10
MESSAGE_BATCH_TIMEOUT=0.5

# Development Configuration
DEBUG=false
RELOAD=false

# Docker Configuration
COMPOSE_PROJECT_NAME=tradingagents-web
